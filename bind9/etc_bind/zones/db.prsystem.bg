;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	prsystem.bg. root.localhost. (
			      4		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;


; name servers - NS records

	IN      NS      ns1.prsystem.bg.


; name servers - A records

prsystem.bg.          IN      A       192.168.1.111
ns1.prsystem.bg.          IN      A       192.168.1.111
server.prsystem.bg.	IN	A	192.168.1.111
